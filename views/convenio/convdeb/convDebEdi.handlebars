<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Débitos de Convênios Edição</title>
	</head>
	<body class="no-skin">
		<div class="main-container ace-save-state" id="main-container">
			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>
							<li class="active">Débitos de Convênios Edição</li>
						</ul><!-- /.breadcrumb -->
					</div>

					<div class="row">
						<div class="col-xs-12">
							<!-- PAGE CONTENT BEGINS -->
							<form class="form-horizontal" role="form"  action="/menu/convenio/convdeb/atualizar" method="POST">

								<div class="col-sm-12"><!-- Widget Dados Básicos -->
									<div class="widget-box">
										<div class="widget-header">
											<h4 class="widget-title">Crédito de Convênios</h4>
											<span class="widget-toolbar">
												<a href="#" data-action="reload">
													<i class="ace-icon fa fa-refresh"></i>
												</a>
												<a href="#" data-action="collapse">
													<i class="ace-icon fa fa-chevron-up"></i>
												</a>
											</span>
										</div>

										<div class="widget-body">
											<div class="widget-main">
												<input name="convdebId" class="hidden" value="{{_id}}"/>
												<label> Convênio Id</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="convdebConvid" id="convdebConvid" placeholder="" class="form-control" value="{{convdeb_convid}}"/>
													</div>
												</div>

												<div class="space space-8"></div>
												<label> Terapia</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="convdebTerapiaid"  id="convdebTerapiaid" placeholder="" class="form-control" value="{{convdeb_terapiaid}}"/>
													</div>
												</div>

												<label for="id-date-picker-1">Data</label>
												<div class="row">
													<div class="col-sm-3">
														<div class="input-group">
															<input name="convdebData" id="convdebData"  type="date" value="{{convdeb_data}}"/>
														</div>
													</div>
												</div>

												<div class="space space-8"></div>
												<label> Valor a Débito</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="convdebValor" id="convdebValor" placeholder="" class="form-control" value="{{convdeb_valor}}"/>
													</div>
												</div>

												<div class="space space-8"></div>
												<label> Observações</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="convdebObs" id="convdebObs" placeholder="" class="form-control" value="{{convdeb_obs}}"/>
													</div>
												</div>

											</div>
										</div>
									</div>
								</div>
							
								<div class="space-6"></div>

								<div class="clearfix form-actions">
									<div class="col-md-offset-3 col-md-9">
										<button class="btn btn-info" name="acao" type="submit">
											<i class="ace-icon fa fa-check bigger-110"></i>
											Salvar
										</button>

										&nbsp; &nbsp; &nbsp;
										<button class="btn btn-warning"  href="/menu/convenio/convdeb/lis">
											<i class="ace-icon fa fa-undo bigger-110"></i>
											Voltar
										</button>
									</div>
								</div>

							</form>
						</div><!-- /.col -->
					</div><!-- /.row -->




				</div>
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
