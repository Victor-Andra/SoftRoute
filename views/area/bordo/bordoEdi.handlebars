			document.getElementById("bordoEscolalia").value = document.getElementById("bordoEscolaliaEdi").value;
			document.getElementById("bordoParticipa").value = document.getElementById("bordoParticipaEdi").value;
			document.getElementById("bordoSegue").value = document.getElementById("bordoSegueEdi").value;
			document.getElementById("bordoRepertorio").value = document.getElementById("bordoRepertorioEdi").value;
		    //Comportamento	
			document.getElementById("bordoAutoagressao").value = document.getElementById("bordoAutoagressaoEdi").value;
			document.getElementById("bordoAgrideadulto").value = document.getElementById("bordoAgrideadultoEdi").value;
			document.getElementById("bordoAgridepares").value = document.getElementById("bordoAgrideparesEdi").value;
			document.getElementById("bordoDestruir").value = document.getElementById("bordoDestruirEdi").value;
			document.getElementById("bordoGritos").value = document.getElementById("bordoGritosEdi").value;
			document.getElementById("bordoChoros").value = document.getElementById("bordoChorosEdi").value;
			document.getElementById("bordoBirras").value = document.getElementById("bordoBirrasEdi").value;
			 //Interação Verbal
			document.getElementById("bordoMandosadulto").value = document.getElementById("bordoMandosadultoEdi").value;
			document.getElementById("bordoMandospares").value = document.getElementById("bordoMandosparesEdi").value;
			document.getElementById("bordoPergunta").value = document.getElementById("bordoPerguntaEdi").value;
			document.getElementById("bordoRespond").value = document.getElementById("bordoRespondEdi").value;
			document.getElementById("bordoImita").value = document.getElementById("bordoImitaEdi").value;
			document.getElementById("bordoInteracaoverbal").value = document.getElementById("bordoInteracaoverbalEdi").value;
