				];

				// Iterar sobre os campos de texto
				pergunta.forEach(function (campo) {
					// Obter o valor do campo de texto
					let op;
					const valorCampo = document.getElementById("atec"+campo+"temp").value;
					if (valorCampo === "1,0" ) {
