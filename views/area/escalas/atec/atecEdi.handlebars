			function checkBoxAoCarregarFormulario() {
				// Obtém o valor do campo ataXxiii01temp
				const valorTemp = document.getElementById("atecConcluidoEdi").value;

				// Obtém referências aos radio buttons
				const checkboxSim = document.getElementById("atecConcluido");
				

				// Verifica o valor e marca o radio button correspondente
				if (valorTemp === "on" ) {
					checkboxSim.checked = true;
					
					console.log(valorTemp);
				} else {
					
					checkboxSim.checked = false;
				}
			}

