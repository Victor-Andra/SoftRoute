			function formatarNumero(valor) {
				return valor.toLocaleString('pt-BR', { minimumFractionDigits: 2, maximumFractionDigits: 2 });
			}
			
			
			// Função para atualizar os campos de texto com os valores dos radio inputs
			function atualizarCampos(questionario, pergunta, valor) {
				// Construa o ID do campo de texto com base no questionário e na pergunta
				var campoId = 'atec' + questionario + 'Q' + pergunta + 'temp';
				console.log(campoId);
				// Encontre o elemento de input de texto com o ID correspondente
				var campoTexto = document.getElementById(campoId);
				console.log(campoTexto.id+"-"+campoTexto.name);
				// Verifique se o campo de texto foi encontrado
				if (campoTexto) {
					// Atualize o valor do campo de texto com o valor do radio input
					campoTexto.value = valor;
				}
				console.log(valor);
			}

