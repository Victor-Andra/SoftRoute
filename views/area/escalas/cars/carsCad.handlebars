		<script type="text/javascript">
			function atualizarTotais() {
				
				// Inicializa o total geral
				let totalGeral = 0;

				// Itera sobre cada categoria
				categorias.forEach(categoria => {
					// Obtém todos os radios da categoria
					const radios = document.querySelectorAll('.cars' + categoria);

					// Inicializa o total da categoria
					let totalCategoria = 0;

					// Itera sobre os radios da categoria
					radios.forEach(radio => {
						// Adiciona o valor do radio ao total da categoria se estiver selecionado
						if (radio.checked) {
							totalCategoria += parseFloat(radio.value);
						}
					});

					// Atualiza o input total da categoria
					document.getElementById('Tcars' + categoria).value = totalCategoria.toFixed(2);

					// Adiciona o total da categoria ao total geral
					totalGeral += totalCategoria;
				});

				// Atualiza o input total geral
				document.getElementById('TTCars').value = totalGeral.toFixed(2);
			}
			
		
		</script>
