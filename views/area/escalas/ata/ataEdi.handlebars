			document.getElementById("ataAcompanhausuid").value = document.getElementById("ataAcompanhausuidEdi").value;
			document.getElementById("ataCorrecusuid").value = document.getElementById("ataCorrecusuidEdi").value;
			document.getElementById("ataCorrecdata").value = document.getElementById("ataCorrecdataEdi").value;
			document.getElementById("ataCorrec").value = document.getElementById("ataCorrecEdi").value;
			//Atualiza o radio button
			checkRadioAoCarregarFormulario();
			// Lista de sufixos a serem removidos para obter os IDs originais
			const sufixos = ["temp"];
			// Loop para percorrer os sufixos
			for (const sufixo of sufixos) {
				// Seleciona todos os elementos que terminam com o sufixo
				const elementosTemp = document.querySelectorAll(`[id$=${sufixo}]`);

				// Preenche os campos correspondentes removendo o sufixo
				elementosTemp.forEach(elementoTemp => {
					const idOriginal = elementoTemp.id.replace(sufixo, "");
					const elementoOriginal = document.getElementById(idOriginal);

					if (elementoOriginal && elementoTemp.value === "true") {
						elementoOriginal.checked = true; // Marca o checkbox se o valor for "true"
					} else {
						elementoOriginal.value = elementoTemp.value; // Preenche outros tipos de elementos
					}
				});
			}
			//atualiza calculos
			atualizarContagem();
			}
