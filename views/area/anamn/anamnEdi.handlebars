		function formataData(date) {//yyyy-MM-dd
			var d = new Date(date);
			if (d.getTimezoneOffset() == 180){
				d.setHours(d.getHours()+3);
			}
			console.log("d:"+d);
				month = '' + (d.getMonth() + 1);
				day = '' + d.getDate();
				year = d.getFullYear();

			if (month.length == 1) {
				month = '0' + month;
			}
				
			if (day.length == 1) {
				day = '0' + day;
			}

			return [year, month, day].join('-');
		}

