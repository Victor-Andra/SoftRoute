				function atualizaData(){
					var hora = "00:00"
					var data = document.getElementById("anamndatacad").value+"T"+hora+":00.000Z";
					var datafmt = new Date(data);
					document.getElementById("anamndatacad").value = datafmt.toISOString();
				}
				function OcultaMsg(){
					document.getElementById("alertMsgFlash").hidden = true;
				}
				function atualizaData(){
					var dataIso = document.getElementById("b").value+"T00:00:00.000Z";
					var dataFmt = new Date(dataIso);
					console.log(dataFmt.toISOString());
					document.getElementById("a").value = dataFmt;
				}
				function formataData(date) {//yyyy-MM-dd
					var d = new Date(date);
					if (d.getTimezoneOffset() == 180){
						d.setHours(d.getHours()+3);
					}
					console.log("d:"+d);
						month = '' + (d.getMonth() + 1);
						day = '' + d.getDate();
						year = d.getFullYear();

					if (month.length == 1) {
						month = '0' + month;
					}
						
					if (day.length == 1) {
						day = '0' + day;
					}

					return [year, month, day].join('-');
				}
				function atualizaDadosbene(){
					let beneid = document.getElementById("anamnBeneid").value;

					var dataIso = document.getElementById(beneid+"_anamnBenedatanasc").value;
					if (dataIso != undefined && dataIso != "undefined"){
						var dataFmt = new Date(dataIso);
						document.getElementById("anamnBenedatanasc").value = formataData(dataFmt);//document.getElementById("_beneDatanasc").value;
						console.log("dataFmt:"+dataFmt)
						document.getElementById("anamnBeneidade").value = ((new Date()).getFullYear() - dataFmt.getFullYear()).toString() + " anos e " + (dataFmt.getMonth()+1).toString() + " meses.";
					}
					document.getElementById("anamnMaenome").value = document.getElementById(beneid+"_beneMae").value;
					document.getElementById("anamnPainome").value = document.getElementById(beneid+"_benePai").value;
				}
			</script>
