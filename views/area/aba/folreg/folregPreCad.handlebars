				document.getElementById("folregSetnum").value = document.getElementById("setNum").value;
				document.getElementById("progsetDesc").value = document.getElementById("setDescr").value;
				document.getElementById("progsetObs").value = document.getElementById("setObs").value;
				document.getElementById("folregQtest").value = document.getElementById("qtEst").value;
				//document.getElementById("folregTiporeg").value = document.getElementById("tiporeg").value;

				document.getElementById("folregEsta").value = document.getElementById("folrega").value;
				document.getElementById("folregEstb").value = document.getElementById("folregb").value;
				document.getElementById("folregEstc").value = document.getElementById("folregc").value;
				document.getElementById("folregEstd").value = document.getElementById("folregd").value;
