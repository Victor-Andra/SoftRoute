			function mudaRegistro() {
				let tipodoc = document.getElementById("folregTiporeg").value;
				console.log(tipodoc)
				if (tipodoc == "Não Informado") {
					document.getElementById('simples').style.display = 'none';
					document.getElementById('duplo').style.display = 'none';
					document.getElementById('chart-Simples').style.display = 'none';
					document.getElementById('chart-Duplo').style.display = 'none';
				} else if (tipodoc == "Duplo") {
					document.getElementById('duplo').style.display = 'block';
					document.getElementById('simples').style.display = 'none';
					document.getElementById('chart-Simples').style.display = 'none';
					document.getElementById('chart-Duplo').style.display = 'block';
				} else if (tipodoc == "Simples") {
					document.getElementById('duplo').style.display = 'none';
					document.getElementById('simples').style.display = 'block';
					document.getElementById('chart-Simples').style.display = 'block';
					document.getElementById('chart-Duplo').style.display = 'none';
				} else {
					document.getElementById('simples').style.display = 'none';
					document.getElementById('duplo').style.display = 'none';
					document.getElementById('chart-Simples').style.display = 'none';
					document.getElementById('chart-Duplo').style.display = 'none';
				}
				document.getElementById('folregTiporeg').ReadOnly
   			}
