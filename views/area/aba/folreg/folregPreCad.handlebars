				document.getElementById("folregEstg").value = document.getElementById("folregg").value;
				document.getElementById("folregEsth").value = document.getElementById("folregh").value;
				document.getElementById("folregEsti").value = document.getElementById("folregi").value;

				showStimulusFieldsWithValues();
				updateStimulusFields();
			}
		</script>
	</head>

