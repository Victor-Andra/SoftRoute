		function showStimulusFieldsWithValues() {
				var fields = ['folregEsta', 'folregEstb', 'folregEstc', 'folregEstd', 'folregEste', 'folregEstf', 'folregEstg', 'folregEsth', 'folregEsti'];
				
				fields.forEach(function(fieldId) {
					var field = document.getElementById(fieldId);
					if (field && field.value.trim() !== '') {
						var stimulusDiv = document.getElementById('stimulus' + fieldId.slice(-1).toUpperCase());
						if (stimulusDiv) {
							stimulusDiv.style.display = 'block';
						}
					}
				});
			}
			function carregaRamdomico() {
				const qtestValue = parseInt(document.getElementById('folregQtest').value, 10);
				const alphabet = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ';
				
				if (isNaN(qtestValue) || qtestValue < 1 || qtestValue > 9) {
					console.error('Invalid value for folregQtest. Please provide a number between 1 and 9.');
					return;
				}

				const letters = alphabet.slice(0, qtestValue).split('');
				const shuffledLetters = shuffleArray(letters);
				
				const tableRows = document.querySelectorAll('#dynamic-table tbody tr');
				
				let letterIndex = 0;
				tableRows.forEach((row, index) => {
					if (index < tableRows.length - 1) { // Skip the last row which is the totals row
						const raCell = row.querySelector('td:nth-child(2) .RA');
						raCell.innerText = shuffledLetters[letterIndex];
						letterIndex = (letterIndex + 1) % shuffledLetters.length;
					}
				});
			}

			function shuffleArray(array) {
				const newArray = array.slice();
				for (let i = newArray.length - 1; i > 0; i--) {
					const j = Math.floor(Math.random() * (i + 1));
					[newArray[i], newArray[j]] = [newArray[j], newArray[i]];
				}
				return newArray;
			}
			
			function updateStimulusFields() {
				var select = document.getElementById('folregQtest');
				var value = parseInt(select.value, 10);

				// Hide all stimulus fields
				var fields = document.querySelectorAll('.stimulus-field');
				fields.forEach(function(field) {
					field.style.display = 'none';
				});

				// Show only the number of fields selected
				for (var i = 0; i < value; i++) {
					document.getElementById('stimulus' + String.fromCharCode(65 + i)).style.display = 'block';
				}

			};
