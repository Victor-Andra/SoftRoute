			// Função para popular os checkboxes
			function populateCheckboxes(containerId, inputId) {
				// Obtém o valor do campo de entrada
				const nomesdicas = document.getElementById(inputId).value;

				// Converte a string em um array
				const progDicatipotext = nomesdicas.split(',');

				// Seleciona todos os divs de checkbox dentro do <td> especificado
				const checkboxes = document.querySelectorAll(`#${containerId} .checkbox`);

				// Itera sobre os checkboxes
				checkboxes.forEach((checkbox, index) => {
					// Se o índice for menor que o comprimento do array, mostra e define o texto
					if (index < progDicatipotext.length) {
						const label = checkbox.querySelector('.label');
						label.textContent = progDicatipotext[index];
						checkbox.style.display = 'block';
					} else {
						// Caso contrário, esconde o checkbox
						checkbox.style.display = 'none';
					}
				});
			}

			// Chama a função para popular os checkboxes após o carregamento do DOM
			document.addEventListener('DOMContentLoaded', () => {
				populateCheckboxes('checkboxContainer1', 'progDicatipotext1');
