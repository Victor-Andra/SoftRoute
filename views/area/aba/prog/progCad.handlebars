		<script type="text/javascript">
			$(document).ready(function(){
				$(".chosen-select-especial").chosen({allow_single_deselect:true});
			});

			function atualizaDicaproced(valor) {
				var valorDicaproced = valor;
				document.getElementById("progDicaproced").value = "";
				document.getElementById("progDicaproced").value = valorDicaproced;
			}

			function atualizarMateriais() {
				var checkboxes = document.querySelectorAll('input[type="checkbox"][id^="progMateriais"]');
				var valoresSelecionados = [];

				checkboxes.forEach(function(checkbox) {
					if (checkbox.checked) {
						valoresSelecionados.push(checkbox.nextElementSibling.textContent.trim());
					}
				});

				document.getElementById('progMateriais').value = valoresSelecionados.join(", ");
			}

			function atualizarEstrutura() {
				var checkboxes = document.querySelectorAll('input[type="checkbox"][id^="progEstrutura"]');
				var valoresSelecionados = [];

				checkboxes.forEach(function(checkbox) {
					if (checkbox.checked) {
						valoresSelecionados.push(checkbox.nextElementSibling.textContent.trim());
					}
				});

				document.getElementById('progEstrutura').value = valoresSelecionados.join(", ");
			}

			function removeExcecaoDicaid(id){
				if (document.getElementById("progDicatipo").value.includes(",")){
					document.getElementById("progDicatipo").value = (document.getElementById("progDicatipo").value.replace((","+id+""), "").replace((""+id+","), "").replace((",,"), ","));
					document.getElementById(id+"_cloneExceTera").remove();
				} else {
					document.getElementById("progDicatipo").value = "";
					document.getElementById(id+"_cloneExceTera").remove();
				}
				
				if (document.getElementById("progDicatipo").value == ""){
					document.getElementById("spanShowExceTera").style = "display:''";
				}
			}
			function removeExcecaoDicatext(text) {
				var inputValue = document.getElementById("progDicatipotext").value;
				var items = inputValue.split(",");

				for (var i = 0; i < items.length; i++) {
					if (items[i].trim() === text) {
						items.splice(i, 1);
						break;
					}
				}

				document.getElementById("progDicatipotext").value = items.join(", ");

				var removedElement = document.getElementById(text + "_cloneExceTera");
				if (removedElement) {
					removedElement.remove();
				}

				if (document.getElementById("progDicatipotext").value === "") {
					document.getElementById("spanShowExceTera").style.display = '';
				}
				// Atualizar os progress bars após remover seleções
				var selectedTextValues = progDicatipotextInput.value; // Alteração aqui
				atualizaProgressBars(selectedTextValues); // Alteração aqui
			}



			function atualizaDica() {
				var selectId = "progDicatipo" + "temp";
				var inputId = "progDicatipo";
				var inputTextId = "progDicatipotext";

				var selectedValues = $('#' + selectId).val();
				var formattedValues = selectedValues ? selectedValues.join(',') : '';

				$('#' + inputId).val(formattedValues);

				// Atualiza o valor do campo progDicatipotext com os nomes das opções selecionadas
				var selectedOptionsText = [];
				$("#" + selectId + " option:selected").each(function() {
					selectedOptionsText.push($(this).text());
				});
				$("#" + inputTextId).val(selectedOptionsText.join(", "));

				var selectedTextValues = $('#' + inputTextId).val(); // Alteração aqui
				atualizaProgressBars(selectedTextValues); // Alteração aqui
			}

			function addExcecaoDicaid() {
			document.getElementById("spanShowExceTera").style = "display:none";

			var id = document.getElementById("progDicatiposelect").value;
			var progSubid = document.getElementById("progDicatipo");
			var span = document.getElementById(id + "_spanExce").cloneNode(true);

			// Obtendo o nome da opção selecionada
			var selectedOptionText = $("#progDicatiposelect option:selected").text();

			span.id = "" + id + "_cloneExceTera";
			document.getElementById("divExcecaoDica").append(span);

			if (progSubid.value == "") {
				progSubid.value = "" + id + "";
			} else {
				progSubid.value = "" + progSubid.value + "," + id + "";
			}

			// Adicionando o nome da opção à div progDicatipotext
			var progDicatipotextInput = document.getElementById("progDicatipotext");
			if (progDicatipotextInput.value == "") {
				progDicatipotextInput.value = selectedOptionText;
			} else {
				progDicatipotextInput.value += ", " + selectedOptionText;
			}

			// Chamando atualizaProgressBars após adicionar uma seleção
			var selectedTextValues = progDicatipotextInput.value; // Alteração aqui
			atualizaProgressBars(selectedTextValues); // Alteração aqui

			// Resetando o valor do select
			document.getElementById("progDicatiposelect").value = "766f69643132333435366964";
		}

		function removeTudo(alvo, clone) {
			let arr = document.getElementById("" + alvo + "").value.split(",");
			arr.forEach((a) => {
				var selectedOptionText = $("#" + a + "_clone" + clone + "Tera").text();

				// Remover o valor correspondente do campo progDicatipotext
				var progDicatipotextInput = document.getElementById("progDicatipotext");
				var currentValue = progDicatipotextInput.value;

				// Remover o texto da opção atual da string de valor atual
				var newValue = currentValue.replace(selectedOptionText, "");

				// Remover qualquer vírgula extra e espaços em branco do novo valor
				newValue = newValue.replace(/,+/g, ",").replace(/,\s*$/, "").trim();

				// Atualizar o valor do campo progDicatipotext
				progDicatipotextInput.value = newValue;

				// Remover o elemento clonado
				$("#" + a + "_clone" + clone + "Tera").remove();
			});

				// Limpar o valor do campo alvo
				document.getElementById("" + alvo + "").value = "";

				// Mostrar novamente o span oculto
				document.getElementById("spanShow" + clone + "Tera").style.display = "";

				// Limpar o valor
				document.getElementById("progDicatipo").value = "";
				document.getElementById("progDicatipotext").value = "";

				// Atualizar os progress bars após remover seleções
				var selectedTextValues = progDicatipotextInput.value; // Alteração aqui
				atualizaProgressBars(selectedTextValues); // Alteração aqui
			}


		function atualizaProgressBars(selectedTextValues) {
			var selectedValues = selectedTextValues.split(", "); // Alteração aqui
			var numSelected = selectedValues.length;
			var procdica = document.getElementById("progDicaproced").value;
			console.log(procdica);
			if (procdica === "" || "Errorless" || "Most-to-Lead"){
			// Definindo os valores para os progress-bars com base no número de seleções e ProcDica sendo diferente de "Least-to-Most"
			var progressValues = [];
			if (numSelected === 1) {
				progressValues = [100];
			} else if (numSelected === 2) {
				progressValues = [70, 30];
			} else if (numSelected === 3) {
				progressValues = [50, 30, 20];
			} else if (numSelected === 4) {
				progressValues = [40, 30, 20, 10];
			} else if (numSelected === 5) {
				progressValues = [35, 25, 20, 15, 5];
			} else if (numSelected === 6) {
				progressValues = [30, 27, 20, 15, 5, 2];
			}
			} else {
			// Definindo os valores para os progress-bars com base no número de seleções e ProcDica sendo igual a "Least-to-Most"
			// O gráfico fica invertido
			if (numSelected === 1) {
				progressValues = [2];
			} else if (numSelected === 2) {
				progressValues = [2, 5];
			} else if (numSelected === 3) {
				progressValues = [2, 5, 15];
			} else if (numSelected === 4) {
				progressValues = [2, 5, 15, 20];
			} else if (numSelected === 5) {
				progressValues = [2,5,15,20,25];
			} else if (numSelected === 6) {
				progressValues = [2, 5, 15, 20, 27, 30];
			}

			}

				// Atualizando os elementos progress-bar
				for (var i = 0; i < 6; i++) {
					var progressBar = $('#porgress' + (i + 1));
					if (i < numSelected) {
						progressBar.show();
						progressBar.find('.progress-value').hide(); // Oculta a porcentagem
						progressBar.find('.progress-bar').css('width', progressValues[i] + '%'); // Define a largura da barra de progresso diretamente
						progressBar.find('.progress-name').text(selectedValues[i]).show();
					} else {
						progressBar.hide();
					}
				}
			}
		
		
			function progressValue(){  
				$('.progress-bar').each(function(){    
					var positionProgressValue = $(this).children('.progress-value').css('left','auto');
					var progressBarValue = $(this).children('.progress-value').html();    
					$(this).width(progressBarValue);    
					if($(this).width() < 21){
					positionProgressValue.css('left','0');
					}    
				});
				}
				progressValue();
				$(window).resize(function(){
				progressValue();
			});

			function carregaBody() {
				document.getElementById("progBeneid").value = document.getElementById("idBene").value;
			}
		</script>
	</head>
