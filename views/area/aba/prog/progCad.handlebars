			}

			h4{
			color: #166d9e;
			}

			h4 .material-icons, .message-read h4 .fa, .message-read h4 .glyphicon {
				margin-right: 15px;
				display: inline-flex;
				vertical-align: middle;
			}

			.material-icons {
				font-size: 20px;
			}

			.progress-title{
				font-size: 14px;
				font-weight: 700;
				margin-bottom: 16px;
				margin-top: 44px;
			}

			.progress-name{
				font-size: 12px;
				font-weight: 700;
				vertical-align: middle;
				
				
			}

			.progress{
				height: 30px;
				background: #e9e9e9;
				border-radius: 0;
				box-shadow: none;
				margin-bottom: 30px;
				overflow: visible;
			}

			.progress .progress-bar{
				box-shadow: none;
				position: relative;
				-webkit-animation: animate-positive 2s;
				animation: animate-positive 2s;
				height: 30px;
			}

			.progress .progress-value{
				font-size: 15px;
				font-weight: bold;
				color: #000;
				position: absolute;
				top: -18px;
				right: 0;
			}

			.progress.pink{
			.progress-bar{
				background: #FF0084;
				&:after{
				background: #FF7E55;
				}
			}
			}

			.progress.red{
			.progress-bar{
				background: #ff5555;
				&:after{
				background: #FF7E55;
				}
			}
			}

			.progress.purple{
			.progress-bar{
				background: #800080;
				&:after{
				background: #FF7E55;
				}
			}
			}
			.progress.green{
			.progress-bar{
				background: #2FB399;
				&:after{
				background: #2FB399;
				}
			}
			}
			.progress.orange{
			.progress-bar{
				background: #FFa500;
				&:after{
				background: #2FB399;
				}
			}
			}

			@-webkit-keyframes animate-positive{
				0% { width: 0; }
			}

			@keyframes animate-positive{
				0% { width: 0; }
			}
			.col-sm-7 input[type="text"] {
				width: 100%;
			}
			
			.spanAzul {
				display: inline-block;
				position: relative;
				font-weight: 400;
				vertical-align: baseline;
				white-space: nowrap;
				background-color: #91b8d0;color: #fff;
