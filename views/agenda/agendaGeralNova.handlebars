			function carregaBody(){
				console.log("Carregando o bode")
				let dias = ["seg","ter","qua","qui","sex"];
				try{
					dias.forEach((dia)=>{
						console.log("dia:"+dia)
						document.getElementsByName(dia).forEach((e)=>{
							console.log("e")
							console.log(e.value)
							let id = "hora"+e.id;
							let valor = e.value;
							console.log("id:"+id)
							console.log("valor:"+valor)
							let val = document.getElementById(valor);
							document.getElementById(id).append(val);

							//teste 
							let dis = ""+dia+"c";
							document.getElementsByName(dis).forEach((e)=>{
								let id = "hora"+e.id;
								console.log(e.id)
								document.getElementById(id).append(e);
							});

							dis = ""+dia+"e";
							document.getElementsByName(dis).forEach((e)=>{
								let id = "hora"+e.id;
								console.log(e.id)
								document.getElementById(id).append(e);
							});
							dis = ""+dia+"d";
							document.getElementsByName(dis).forEach((e)=>{
								let id = "hora"+e.id;
								console.log(e.id)
								document.getElementById(id).append(e);
							});
						})
					})
					
				} catch(err){
					console.log(err)
				}
			}
			function formataData(){
				let dat = new Date(document.getElementById('dataFil').value);
				let mes = (dat.getMonth()+1).toString();
				let dia = (dat.getUTCDate()).toString();
				if (mes.length == 1){
					mes = "0"+mes;
				}
				if (dia.length == 1){
					dia = "0"+dia;
				}
				let fulldate=(dat.getFullYear()+"-"+mes+"-"+dia+' 00:00:00.000Z').toString();
				document.getElementById('dataFinal').value = fulldate;
				console.log("dataFinal:"+document.getElementById('dataFinal').value);
			}
		</script>
