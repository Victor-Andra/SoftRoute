		<script type="text/javascript">
			function controlaCategoria(){
				let origem = document.getElementById("agendaOrg").value;
				if(origem == "Padrão"){
					document.getElementById("agendaCateg").value = "Padrão";
					document.getElementById("Padrão").style.display = "";
					document.getElementById("Falta").style.display = "none";
					document.getElementById("Apoio").style.display = "none";
					document.getElementById("Supervisão").style.display = "none";
					document.getElementById("Pais").style.display = "none";
					document.getElementById("Glosa").style.display = "none";
					document.getElementById("Extra").style.display = "none";
					document.getElementById("SubstitutoFixo").style.display = "none";
				} else {
					document.getElementById("agendaCateg").value = "Apoio";
					document.getElementById("Padrão").style.display = "none";
					document.getElementById("Falta").style.display = "";
					document.getElementById("Apoio").style.display = "";
					document.getElementById("Supervisão").style.display = "";
					document.getElementById("Pais").style.display = "";
					document.getElementById("Glosa").style.display = "";
					document.getElementById("Extra").style.display = "";
					document.getElementById("SubstitutoFixo").style.display = "";
				}
				this.mudaSubis();
			}
			function selectConv(){
				var v1 = document.getElementById(document.getElementById("agendaBeneid").value).value.toString();
				console.log(v1)
				document.getElementById("agendaConvid").value = v1;
			}
			function atualizaConvId(){
				let convId = document.getElementById("agendaConvid").value;
				console.log(convId)
			}
			function carregaBody(){
				let data= new Date();
				let mes = (data.getMonth()+1).toString();
				let dia = (data.getUTCDate()).toString();
				if (mes.length == 1){
					mes = "0"+mes;
				}
				if (dia.length == 1){
					dia = "0"+dia;
				}
				let fulldate=(data.getFullYear()+"-"+mes+"-"+dia).toString();
				document.getElementById("agendaData").value = fulldate;
				document.getElementById("agendaExtra").value = "false";
				document.getElementById("agendaExtra").checked = true;
				document.getElementById("agendaExtra").checked = false;
				console.log("carregou!");
				
				// construtor da tabela de exceções
				//Constroi as Tags dentro da div de Terapeutas e Substitutos
				let arr = document.getElementsByName("tagContainerTerapeuta");

				arr.forEach((a)=>{
					let novo_vetor;
					let input = document.getElementById(a.id + "_input");
					if (input && input.value){
						console.log("input.value: "+input.value)
						novo_vetor = input.value.split(",");
						novo_vetor.forEach((e)=>{
							let excecao = document.getElementById(e+"_span");
							a.append(excecao.cloneNode(true));
						})
					}
				})

				let arrBene = document.getElementsByName("beneid");
				arrBene.forEach((bene)=>{
					let beneval = bene.value;
					let inputExcecao = document.getElementsByName("idBeneExceTera_"+beneval);

					if (inputExcecao){
						inputExcecao.forEach((exec)=>{
							let idExcecao = exec.value;
							let idsExceTera = document.getElementById(idExcecao+"_idsExceTera");
							if (idsExceTera && idsExceTera.value){
								let arr = idsExceTera.value;
								if (arr.includes(",")){
									let arrIdTera = arr.split(",");
									arrIdTera.forEach((tera)=>{
										let elementoClonado = document.getElementById(tera+"_span").cloneNode(true);
										document.getElementById("containerExecaoTerapeuta_"+idExcecao+"_"+beneval).append(elementoClonado);
									})
								} else {
									let elementoClonado = document.getElementById(idsExceTera.value+"_span").cloneNode(true);
									document.getElementById("containerExecaoTerapeuta_"+idExcecao+"_"+beneval).append(elementoClonado);
								}
							}
						})
					}

					let inputSubstituto = document.getElementsByName("idBeneSubsTera_"+beneval);

					if (inputSubstituto && inputSubstituto != ""){
						inputSubstituto.forEach((subs)=>{
							let idSubstituto = subs.value;
							let idsSubsTera = document.getElementById(idSubstituto+"_idsSubsTera");
							if (idsSubsTera && idsSubsTera.value) {
								let arr = idsSubsTera.value;
								if (arr.includes(",")){
									let arrIdSubs = arr.split(",");
									arrIdSubs.forEach((subs)=>{
										let elementoClonadosubs = document.getElementById(subs+"_span").cloneNode(true);
										document.getElementById("containerSubsTerapeuta_"+idSubstituto+"_"+beneval).append(elementoClonadosubs);
									})
								} else {
									let elementoClonadosubs = document.getElementById(idsSubsTera.value+"_span").cloneNode(true);
									document.getElementById("containerSubsTerapeuta_"+idSubstituto+"_"+beneval).append(elementoClonadosubs);
								}
							}
						})
					}
				});
			}
			
			function alteraMigrado(){
				let chck = document.getElementById("agendaMigrado");
				if(chck.checked){
					chck.value="true";
					console.log(chck.value);
				} else {
					chck.value="false";
					console.log(chck.value);
				}
			}
			function alteraCheck(checkboxid, elementoid){
				let chck = document.getElementById(checkboxid);
				let elemento = document.getElementById(elementoid);
				if(chck.checked){
					elemento.value="true";
				} else {
					elemento.value="false";
				}
			}
			function mudaSubis() {
				let tipodoc = document.getElementById("agendaCateg").value;
				console.log(tipodoc);
				if (tipodoc == "SubstitutoFixo") {
					document.getElementById('widgetboxSubs').style.display = 'block';
				} else {
					document.getElementById('widgetboxSubs').style.display = 'none';
					
				}
   			}
			//atualiza a tabela de sessões
			function selectSessao(){
				let beneId = document.getElementById("agendaBeneid").value;
				
				for (child of document.getElementById("blocoSessoes").children){
					child.remove();
				}
				
				document.getElementById("blocoSessoes").name = beneId;

				if (document.getElementById(beneId+"_sessao") != null){
					let elementoClonado = document.getElementById(beneId+"_sessao").cloneNode(true);
					console.log(beneId+"_sessao");
					elementoClonado.id = elementoClonado.id + "_clone";
					document.getElementById("blocoSessoes").append(elementoClonado);
				}
			}
			//atualiza a tabela de Excecões
			function selectExcecao() {
				document.getElementById("blocoExcecaos").innerHTML = "";
				document.getElementById("blocoExcecaos").innerHTML = "";
				let beneId = document.getElementById("agendaBeneid").value;
				let tableClone = document.getElementById(beneId+"_divBeneExcecao").cloneNode(true);
				document.getElementById("blocoExcecaos").append(tableClone);
				
			}
		</script>
