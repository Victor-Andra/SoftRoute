<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Pagina de Erro!</title>
	</head>

	<body onload="javascript:controlaNave();" class="no-skin">


		<div class="main-container ace-save-state" id="main-container">


			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>

							<li class="active">Erro!</li>
						</ul><!-- /.breadcrumb -->

					</div>
					<!--//MENSAGEM FLASH-->
					{{#compareString flash.sucesso "true"}}
							<div id="alertMsgFlash" name="alertMsgFlash" class="alert alert-block alert-success">
								<p>
									<strong>
										<i class="ace-icon fa fa-check"></i>
										{{flash.texto}}
									</strong>
									<button type="button" class="close" onclick="javascript:OcultaMsg()">
										<i class="ace-icon fa fa-times"></i>
									</button>
								</p>
							</div>
							<!--<div id="alertMsgFlash" name="alertMsgFlash" class="alert alert-success">{{flash.texto}}<button onclick="javascript:OcultaMsg()">X</button></div>-->
							{{else}}
								{{#compareString flash.sucesso "false"}}
									<div id="alertMsgFlash" name="alertMsgFlash" class="alert alert-danger">
										<p>
											<strong>
												<i class="ace-icon fa fa-times"></i>
												{{flash.texto}}
											</strong>
											<button type="button" class="close" onclick="javascript:OcultaMsg()">
												<i class="ace-icon fa fa-times"></i>
											</button>
										</p>
									</div>
									<!--<div id="alertMsgFlash" name="alertMsgFlash" class="alert alert-danger">{{flash.texto}}<button onclick="javascript:OcultaMsg()">X</button></div>-->
								{{else}}
								{{/compareString}}
							{{/compareString}}
					<!--//MENSAGEM FLASH-->
								<div class="error-container">
									<div class="well">
										<h1 class="grey lighter smaller">
											<span class="blue bigger-125">
												<i class="ace-icon fa fa-random"></i>
												500
											</span>
											Opa! algo deu errado!
										</h1>

										<hr>
										<h3 class="lighter smaller">
											
											<i class="ace-icon fa fa-wrench icon-animated-wrench bigger-125"></i>
											&nbsp;&nbsp;Vamos cuidar disso!
										</h3>

										<div class="space"></div>

										<div>
											<h4 class="lighter smaller">Você também pode:</h4>

											<ul class="list-unstyled spaced inline bigger-110 margin-15">
												<li>
													<i class="ace-icon fa fa-hand-o-right blue"></i>
													Leia a ajuda!
												</li>

												<li>
													<i class="ace-icon fa fa-hand-o-right blue"></i>
													Mais informações sobre o erro ocorrido!
												</li>

												<li>
													<i class="ace-icon fa fa-hand-o-right blue"></i>
													Equipe de Desenvolvimento!<br>
													Wagner Freitas: WhatsApp 81 9 9898 59 34<br>
													Victor Cintra: WhatsApp 81 9 9991 53 32<br>

												</li>

												
											</ul>
										</div>

										<hr>
										<div class="space"></div>

										<div class="center">
											<a href="javascript:history.back()" class="btn btn-grey">
												<i class="ace-icon fa fa-arrow-left"></i>
												Voltar
											</a>

											<a href="#" class="btn btn-primary">
												<i class="ace-icon fa fa-tachometer"></i>
												Dashboard
											</a>
										</div>
									</div>
								</div>
				</div>
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
