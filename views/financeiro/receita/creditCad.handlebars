<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Receitas</title>
	</head>

	<body onload="javascript:controlaNave();" class="no-skin">


		<div class="main-container ace-save-state" id="main-container">


			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>

							<li class="active">Receitas Cadastro</li>
						</ul><!-- /.breadcrumb -->

					</div>

					<div class="row">
						<div class="col-xs-12">
							<!-- PAGE CONTENT BEGINS -->
							<form class="form-horizontal" role="form" action="/menu/financeiro/receita/add" method="POST">


								<div class="col-sm-12"><!-- Widget Dados Básicos -->
									<div class="widget-box">
										<div class="widget-header">
											<h4 class="widget-title">Dados</h4>

											<span class="widget-toolbar">

												<a href="#" data-action="reload">
													<i class="ace-icon fa fa-refresh"></i>
												</a>

												<a href="#" data-action="collapse">
													<i class="ace-icon fa fa-chevron-up"></i>
												</a>

											</span>
										</div>

										<div class="widget-body">
											<div class="widget-main">
												<div class="hidden" class="col-sm-6">
													<input type="text" name="contarec_usuidcad" id="contarec_usuidcad" placeholder="" class="form-control" />
												</div>
												<div class="hidden" class="col-sm-6">
													<input type="text" name="contarecDatacad" id="contarecDatacad" placeholder="" class="form-control" />
												</div>

  
												<label> Número NF</label>
												<div class="form-group">
													<div class="col-sm-2">
														<input type="text" name="contarecNfnum" id="contarecNfnum" placeholder="" class="form-control" />
													</div>
												</div>

												<label> Beneficiário</label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecBeneid" id="contarecBeneid" placeholder="" class="form-control" />
													</div>
												</div>

												<label> Tomador</label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecBenenome" id="contarecBenenome" placeholder="" class="form-control" />
													</div>
												</div>

												

												<label> Cpf / Cnpj</label>
												<div class="form-group">
													<div class="col-sm-2">
														<input type="text" name="contarecCpfcnpj" id="contarecCpfcnpj" placeholder="" class="form-control" />
													</div>
												</div>
												<label> Convenio</label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecConvid" id="contarecConvid" placeholder="" class="form-control" />
													</div>
												</div>

												<div>
													<label>Data Evento</label>
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-calendar"></i>
														</span>
														<input name="contarecDataevento" id="contarecDataevento" class="col-sm-4" class="form-control" type="text" id="creditDatapg" />
													</div>
												</div>

				
												<div>
													<label>Data Vencimento</label>
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-calendar"></i>
														</span>
														<input name="contarecDatavenci" id="contarecDatavenci" class="col-sm-4" class="form-control" type="text" id="creditDatapg" />
													</div>
												</div>

											
												<div>
													<label>Data Pagamento</label>							
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-calendar"></i>
														</span>
									
														<input name="contarecDatapg" id="contarecDatapg" class="col-sm-4" class="form-control" type="text" id="creditDatapg" />
													</div>
												</div>

				
												<label> Valor Previsto </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlrprev" id="contarecVlrprev" placeholder="" class="form-control" value="0,00"/>
													</div>
												</div>


												<label> Valor Juros </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlrjuros" id="contarecVlrjuros" placeholder="0,00" class="form-control" value="0,00"/>
													</div>
												</div>


												<label> Valor Multa </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlrmulta" id="contarecVlrmulta" placeholder="0,00" class="form-control" value="0,00"/>
													</div>
												</div>

												<div>
													<label>Adiantamento</label>
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-calendar"></i>
														</span>
														<input name="contarecDataadianta" id="contarecDataadianta" class="col-sm-4" class="form-control" type="text" id="creditDatapg" />
													</div>
												</div>

												<label> Valor Adiantado </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlradianta" id="contarecVlradianta" placeholder="0,00" class="form-control" value="0,00"/>
													</div>
												</div>

												<div>
													<label>Adiantamento</label>
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-calendar"></i>
														</span>
														<input name="contarecDataadianta" id="contarecDataadianta" class="col-sm-4" class="form-control" type="text" id="creditDatapg" />
													</div>
												</div>

												<label> Valor Diferença </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlrdiferenca" id="contarecVlrdiferenca" placeholder="0,00" class="form-control" value="0,00"/>
													</div>
												</div>


												<label> Valor Pago </label>
												<div class="form-group">
													<div class="col-sm-3">
														<input type="text" name="contarecVlrpg" id="contarecVlrpg" placeholder="0,00" class="form-control" value="0,00"/>
													</div>
												</div>

												<label> Forma de Pagamento</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="contarecVlrtipo" id="contarecVlrtipo" placeholder="" class="form-control" />
													</div>
												</div>

												<label>Pago?</label>
												<div class="form-group">
														<div class="col-sm-2">
															<select name="contarecPg" id="contarecPg" class="chosen-select" data-placeholder="Escolha...">
																<option value="Não">Não</option>
																<option value="Sim">Sim</option>
															</select>
														</div>
												</div>

												<label> Observações </label>
												<div class="form-group">
													<div class="col-sm-9">
														<input type="text" name="contarecDescr" id="contarecDescr" line="3"  placeholder="" class="form-control" />
													</div>
												</div>


											</div>
										</div>
									</div>
								</div>
							
							
								<div class="space-4"></div>							
								<div class="space-4"></div>
	
									<div class="form-actions">
										<div class="col-xs-12" style="text-align: center;" >
											<button class="btn btn-info"  name="acao" type="submit">
												<i class="ace-icon fa fa-check bigger-110"></i>
												Salvar
											</button>
		
											&nbsp; &nbsp; &nbsp;
											<button class="btn btn-warning"  type="reset">
												<i class="ace-icon fa fa-undo bigger-110"></i>
												Voltar
											</button>
										</div>
									</div>

							</form>


						</div><!-- /.col -->
					</div><!-- /.row -->




				</div>
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
