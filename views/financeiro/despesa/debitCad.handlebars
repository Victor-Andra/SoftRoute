															<option value="Extra">Extra</option>																	
														</select>
													</div>
												</div>
												<div class="col-xs-6">													
													<label>Terapia</label>
													<div class="form-group">
														<div class="col-sm-4" style="width: 600px;">
															<select name="agendaTerapiaid" id="agendaTerapiaid" class="chosen-select" data-placeholder="Escolha..." onchange="javascript:carregaValores()">
																<option value="">&nbsp;</option>
																{{#each terapias}}
																	<option value="{{_id}}">{{terapia_nome}}</option>
																{{/each}}
															</select>
														</div>
													</div>
												</div>
												<label> Convenio</label>
												<div class="form-group">
													<div class="col-sm-6">
														<select id="atendConvid" name="atendConvid" class="chosen-select" data-placeholder="Escolha...">
															<option value="">&nbsp;</option>
															{{#each convs}}
																	<option value="{{_id}}">{{conv_nome}}</option>
															{{/each}}
														</select>
													</div>
												</div>
												<label> Terapeuta</label>
												<div class="form-group">
													<div class="col-sm-6">
														<select id="atendTerapeutaid" name="atendTerapeutaid" class="chosen-select" data-placeholder="Escolha..." onchange="javascript:atualizaTerapeutaId()">
															<option value="">&nbsp;</option>
															{{#each usuarios}}
																<option value="{{_id}}">{{usuario_nome}}</option>
															{{/each}}
														</select>
													</div>
												</div>
												-->
											</div>
										</div>
									</div>
								</div>							
								<div class="space-4"></div>							
								<div class="space-4"></div>
									<div class="form-actions">
										<div class="col-xs-12" style="text-align: center;" >
										<button class="btn btn-info"  name="acao" type="submit">
											<i class="ace-icon fa fa-check bigger-110"></i>
											Salvar
										</button>
										&nbsp; &nbsp; &nbsp;
										<button class="btn btn-warning"  type="reset">
											<i class="ace-icon fa fa-undo bigger-110"></i>
											Voltar
										</button>
									</div>
								</div>
							</form>
						</div><!-- /.col -->
					</div><!-- /.row -->
				</div>
			</div><!-- /.main-content -->
		</div><!-- /.main-container -->
	</body><!-- basic scripts -->
</html>
