<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Empresa</title>
	</head>

	<body onload="javascript:controlaNave();" class="no-skin">


		<div class="main-container ace-save-state" id="main-container">


			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>

							<li class="active">Empresa Cadastro</li>
						</ul><!-- /.breadcrumb -->

					</div>

					<div class="row">
						<div class="col-xs-12">
							<!-- PAGE CONTENT BEGINS -->
							<form class="form-horizontal" role="form" action="/menu/ferramentas/empresa/add" method="POST">


								<div class="col-sm-12"><!-- Widget Dados Básicos -->
									<div class="widget-box">
										<div class="widget-header">
											<h4 class="widget-title">Dados</h4>

											<span class="widget-toolbar">

												<a href="#" data-action="reload">
													<i class="ace-icon fa fa-refresh"></i>
												</a>

												<a href="#" data-action="collapse">
													<i class="ace-icon fa fa-chevron-up"></i>
												</a>

											</span>
										</div>

										<div class="widget-body">
											<div class="widget-main">

												<label> Unidade</label>
												<div class="form-group">
													<div class="col-sm-12">
														<input type="text" name="empresaUnidade" id="empresaUnidade"  class="form-control" />
													</div>
												</div>
				
												<label> Razão Social</label>
												<div class="form-group">
													<div class="col-sm-12">
														<input type="text" name="empresaNome" id="empresaNome"  class="form-control" />
													</div>
												</div>

												<label> Nome Fantasia</label>
												<div class="form-group">
													<div class="col-sm-12">
														<input type="text" name="empresaFantasia" id="empresaFantasia" placeholder="" class="form-control" />
													</div>
												</div>


												<label> CNPJ</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="empresaCnpj" id="empresaCnpj" class="form-control" />
													</div>
												</div>


												<label> Inscrição Municipal</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="empresaMuni" id="empresaMuni" class="form-control" />
													</div>
												</div>

												<label> Inscrição Estadual</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="empresaEstad" id="empresaEstad" class="form-control" />
													</div>
												</div>

												<label > Endereço </label>
												<div class="form-group">
													<div class="col-sm-12">
														<input type="text" name="empresaEnd" id="empresaEnd" class="form-control" />
													</div>
												</div>
				
												<label> Complemento </label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" name="empresaEndcompl" id="empresaEndcompl" class="form-control" />
													</div>
												</div>
				
												<label> Bairro </label>
												<div class="form-group">
													<div class="col-sm-9">
														<input type="text" name="empresaEndbairro" id="empresaEndbairro" class="form-control" />
													</div>
												</div>
				
												<label> Cidade </label>
												<div class="form-group">
													<div class="col-sm-9">
														<input type="text" name="empresaEndcidade" id="empresaEndcidade" class="form-control" />
													</div>
												</div>

												<label>UF</label>
												<div class="form-group">
														<div class="col-sm-4">
															<select name="empresaEnduf"  id="empresaEnduf"  class="chosen-select" data-placeholder="Escolha...">
																	<option value="">&nbsp;</option>
																	{{#each estados}}
																	<option value="{{_id}}">{{estado_uf}}</option>
																	{{else}}
																	{{/each}}
															</select>
														</div>
												</div>
				
												<label>CEP
													<small class="text-warning">00000-000</small>
												</label>
												<div class="form-group">
													<div class="col-sm-4">
														<input type="text" name="empresaEndcep" id="empresaEndcep" class="form-control" />
													</div>
												</div>


												<div>
													<label>whatsApp
														<small class="text-warning">(99) 9999-9999</small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-phone"></i>
														</span>
									
														<input name="empresaWhatsapp" id="empresaWhatsapp" class="col-sm-4" class="form-control input-mask-phone" type="text" id="Whatsapp" />
													</div>
												</div>
				
									
												<div>
													<label>Celular
														<small class="text-warning"> (99) 9999-9999</small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-phone"></i>
														</span>
									
														<input name="empresaCel" id="empresaCel" class="col-sm-4" class="form-control input-mask-phone" type="text" id="Celular" />
													</div>
												</div>

												<div>
													<label>Celular
														<small class="text-warning"> (99) 9999-9999</small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-phone"></i>
														</span>
									
														<input name="empresaFixo" id="empresaFixo" class="col-sm-4" class="form-control input-mask-phone" type="text" id="Celular" />
													</div>
												</div>

												<div>
													<label>E-mail
														<small class="text-warning"></small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-envelope-open"></i>
														</span>
									
														<input name="empresaEmail" id="empresaEmail" class="col-sm-4" class="form-control input-mask-phone" type="text" id="Celular" />
													</div>
												</div>


												<div>
													<label>Site
														<small class="text-warning"></small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-home"></i>
														</span>
									
														<input name="empresaSite" id="empresaSite" class="col-sm-4" class="form-control input-mask-phone" type="text" id="Celular" />
													</div>
												</div>
												
												<div>
													<label>Status
														<small class="text-warning"></small>
													</label>
									
													<div class="input-group">
														<span class="input-group-addon">
															<i class="ace-icon fa fa-home"></i>
														</span>
									
														<input name="convStatus" id="convStatus" class="col-sm-4" class="form-control input-mask-phone" type="text" />
													</div>
												</div>

												<label> Observações </label>
												<div class="form-group">
													<div class="col-sm-9">
														<input type="text" name="empresaObs" id="empresaObs" class="form-control" />
													</div>
												</div>


											</div>
										</div>
									</div>
								</div>
							
							
								<div class="space-4"></div>							
								<div class="space-4"></div>
	
								<div class="form-actions">
									<div class="col-md-offset-3 col-md-9">
										<button class="btn btn-info"  name="acao" type="submit">
											<i class="ace-icon fa fa-check bigger-110"></i>
											Salvar
										</button>
	
										&nbsp; &nbsp; &nbsp;
										<button class="btn btn-warning"  href="/menu/admin/branco">
											<i class="ace-icon fa fa-undo bigger-110"></i>
											Voltar
										</button>
									</div>
								</div>

							</form>


						</div><!-- /.col -->
					</div><!-- /.row -->




				</div>
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
