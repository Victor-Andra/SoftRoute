					document.getElementById("extraValorcre").value = document.getElementById(convId+"/"+teraId+"/cre").value;
					document.getElementById("extraValordeb").value = document.getElementById(convId+"/"+teraId+"/deb").value;
					document.getElementById("tabilValorcredit").value = document.getElementById(convId+"/"+teraId+"/cre").value;
					document.getElementById("tabilValordebit").value = document.getElementById(convId+"/"+teraId+"/deb").value;
				} catch(err){
					document.getElementById("extraValorcre").value = "0,00";
					document.getElementById("extraValordeb").value = "0,00";
					document.getElementById("tabilValorcredit").value = "0,00";
					document.getElementById("tabilValordebit").value = "0,00";
				}
			}
			function carregaValoresMerge(){
				let teraId = document.getElementById("extraMergeTerapiaid").value;
				document.getElementById("creditMergeTerapiaid").value = teraId;
				document.getElementById("debitMergeTerapiaid").value = teraId;
				document.getElementById("tabilMergeTerapiaid").value = teraId;
				
				try{
					let convId = document.getElementById("extraConvid").value
					document.getElementById("extraMergevalorcre").value = document.getElementById(convId+"/"+teraId+"/cre").value;
					document.getElementById("extraMergevalordeb").value = document.getElementById(convId+"/"+teraId+"/deb").value;
				} catch(err){
					document.getElementById("extraMergevalorcre").value = "0,00";
					document.getElementById("extraMergevalordeb").value = "0,00";
				}
