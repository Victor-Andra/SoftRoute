<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Atendimentos por Beneficiário</title>
	</head>

	<body class="no-skin">


		<div class="main-container ace-save-state" id="main-container">


			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>

							<li class="active">Atendimentos por Beneficiário</li>
						</ul><!-- /.breadcrumb -->

					</div>

				</div>


				{{#each atends}}
				<div class="row">
					<div class="col-xs-12">
						<!-- PAGE CONTENT BEGINS -->
						<div class="space-6"></div>

						<div class="row">
							<div class="col-sm-10 col-sm-offset-1">
								<div class="widget-box transparent">
									<div class="widget-header widget-header-large">
										<h3 class="widget-title grey lighter">
											<i class="ace-icon fa fa-leaf green"></i>
											Atendimentos por Beneficiário
										</h3>

										<div class="widget-toolbar no-border invoice-info">
											<span class="invoice-info-label">index:</span>
											<span class="red">#1</span>

											<br />
											<span class="invoice-info-label">Período de:</span>
											<span class="blue">01/03/2022 até 30/03/2022</span>
										</div>

										<div class="widget-toolbar hidden-480">
											<a href="#">
												<i class="ace-icon fa fa-print"></i>
											</a>
										</div>
									</div>
									{{#each atends}}
										<div class="widget-body">
											<div class="widget-main padding-24">
												<div class="row">
													<div class="col-sm-12">
														<div class="row">
															<div class="col-xs-12 label label-lg label-info arrowed-in arrowed-right">
																<b>Informações do Prestador</b>
															</div>
														</div>

														<div>
															<ul class="list-unstyled">
																<li>
																	<i class="ace-icon fa fa-caret-right blue"></i><b>Route Centro de Terapia e Apoio Infantil </b>
																</li>
																<li>
																	<i class="ace-icon fa fa-caret-right blue"></i><b>CNPJ: </b> 33.346.423/0001-17
																</li>
															</ul>
														</div>
														

														{{#each atends}}
														<div class="row">
															<div class="col-xs-12 label label-lg label-success arrowed-in arrowed-right">
																<b>Informações do Beneficiário</b>
															</div>
														</div>

														<div>
															<ul class="list-unstyled ">
																<li>
																	<i class="ace-icon fa fa-caret-right green"></i><b>Beneficiário:</b> "{{bene_nome}}"
																</li>
																<li>
																	<i class="ace-icon fa fa-caret-right green"></i><b>Responsável: </b>"{{bene_out}}"
																</li>
																<li>
																	<i class="ace-icon fa fa-caret-right green"></i><b>CPF Responsável:</b>"{{bene_outcpf}}"
																</li>

															</ul>
														</div>
														
														<div class="row">
															<div class="col-xs-12 label label-lg label-warning arrowed-in arrowed-right">
																<b>Informações de Atendimentos</b>
															</div>
														</div>
												</div><!-- /.row -->



												<div>
													<table class="table table-striped table-bordered">
														<thead>
															<tr>
																<th class="center">#</th>
																<th>Data Atendimento</th>
																<th class="hidden-xs">Especialidade</th>
																<th class="hidden-480">Profissional</th>
																
															</tr>
														</thead>
													{{#eache atends}}	
														<tbody>
															<tr>
																<td class="center">1</td>
																<td><a>{{atend_data}}</a></td>
																<td>{{atend_terapiaid}}</td>
																<td>{{atend_uduid}}</td>					
															</tr>

														</tbody>
													{{/each}}
													</table>
												</div>
												
												<div>
													<table class="table table-striped table-bordered">
														<thead>
															<tr>
																
																<th>Data:</th>
																<th>Assinatura:</th>
																
																
															</tr>
														</thead>

														<tbody>
															<tr>
														
																<td><a>01/03/2022</a></td>
																<td> </td>
																	
															</tr>

														</tbody>
													</table>
												</div>
											</div>
										</div>
										</div>
									{{/each}}
								</div>
							</div>
						</div>

						<!-- PAGE CONTENT ENDS -->
					</div><!-- /.col -->
				</div><!-- /.row -->

			
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
