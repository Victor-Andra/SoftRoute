<!DOCTYPE html>
<html lang="pt-br">
	<head>
		<title>Escola</title>
		<script type="text/javascript">
			function carregaBody(){
				console.log("carregando");
				document.getElementById("escolaEnduf").value = document.getElementById("escolaEndufEdi").value;
				console.log("carregado!");
			}
		</script>
	</head>

	<body class="no-skin" onload="javascript:carregaBody()">
		<div class="main-container ace-save-state" id="main-container">
			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>
							<li class="active">Escola Editar</li>
						</ul><!-- /.breadcrumb -->
					</div>
					<div class="row">
						<div class="col-xs-12">
							<!-- PAGE CONTENT BEGINS -->
							<form class="form-horizontal" role="form" action="/menu/beneficiario/escola/atualizar" method="POST">
								<div class="col-sm-12"><!-- Widget Dados Básicos -->
									<div class="widget-box">
										<div class="widget-header">
											<h4 class="widget-title">Escola</h4>

											<span class="widget-toolbar">

												<a href="#" data-action="reload">
													<i class="ace-icon fa fa-refresh"></i>
												</a>

												<a href="#" data-action="collapse">
													<i class="ace-icon fa fa-chevron-up"></i>
												</a>

											</span>
										</div>

										<div class="widget-body">
											<div class="widget-main">
												<label>Nome Escola</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaNome" name="escolaNome" class="form-control" value="{{escola.escola_nome}}"/>
													</div>
												</div>
												<label>Nome Fantasia</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaFantasia" name="escolaFantasia" class="form-control" value="{{escola.escola_fantasia}}"/>
													</div>
												</div>
												<label>Endereço</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaEnd" name="escolaEnd" class="form-control" value="{{escola.escola_end}}"/>
													</div>
												</div>
												<label>Complemento</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaEndcompl" name="escolaEndcompl" class="form-control" value="{{escola.escola_endcompl}}"/>
													</div>
												</div>
												<label>Bairro</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaEndbairro" name="escolaEndbairro" class="form-control" value="{{escola.escola_endbairro}}"/>
													</div>
												</div>
												<label>Cidade</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaEndcidade" name="escolaEndcidade" class="form-control" value="{{escola.escola_endcidade}}"/>
													</div>
												</div>
												<label>UF</label>
												<div class="form-group">
														<div class="col-sm-6">
															<select id="escolaEnduf" name="escolaEnduf" class="chosen-select" data-placeholder="Escolha o estado..." >
																{{#each estados}}
																	<option value="{{_id}}">{{estado_nome}}</option>
																{{/each}}
															</select>
														</div>
												</div>
												<label>CEP</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaEndcep" name="escolaEndcep" class="form-control" value="{{escola.escola_endcep}}"/>
													</div>
												</div>
												<label>Coordenador</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaCoordenador" name="escolaCoordenador" class="form-control" value="{{escola.escola_coordenador}}"/>
													</div>
												</div>
												<label>Whatsapp</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaCoorwhatsapp" name="escolaCoorwhatsapp" class="form-control" value="{{escola.escola_coorwhatsapp}}"/>
													</div>
												</div>
												<label>Professor(a)</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaProfessor" name="escolaProfessor" class="form-control" value="{{escola.escola_professor}}"/>
													</div>
												</div>
												<label>Whatsapp</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaProfwhatsapp" name="escolaProfwhatsapp" class="form-control" value="{{escola.escola_profwhatsapp}}"/>
													</div>
												</div>
												<label>Telefone Fixo</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaFixo" name="escolaFixo" class="form-control" value="{{escola.escola_fixo}}"/>
													</div>
												</div>
												<label>Observações</label>
												<div class="form-group">
													<div class="col-sm-6">
														<input type="text" id="escolaObs" name="escolaObs" class="form-control" value="{{escola.escola_obs}}"/>
													</div>
												</div>
			

												<div class="hidden">
													<input name="escolaId" value="{{escola._id}}" />
													<input id="escolaNomeEdi" name="escolaNomeEdi" value="{{escola.escola_nome}}"/>
													<input id="escolaFantasiaEdi" name="escolaFantasiaEdi" value="{{escola.escola_fantasia}}"/>
													<input id="escolaEndEdi" name="escolaEndEdi" value="{{escola.escola_end}}"/>
													<input id="escolaEndcomplEdi" name="escolaEndcomplEdi" value="{{escola.escola_endcompl}}"/>
													<input id="escolaEndbairroEdi" name="escolaEndbairroEdi" value="{{escola.escola_endbairro}}"/>
													<input id="escolaEndcidadeEdi" name="escolaEndcidadeEdi" value="{{escola.escola_endcidade}}"/>
													<input id="escolaEndufEdi" name="escolaEndufEdi" value="{{escola.escola_enduf}}"/>
													<input id="escolaEndcepEdi" name="escolaEndcepEdi" value="{{escola.escola_endcep}}"/>
													<input id="escolaCoordenadorEdi" name="escolaCoordenadorEdi" value="{{escola.escola_coordenador}}"/>
													<input id="escolaCoorwhatsappEdi" name="escolaCoorwhatsappEdi" value="{{escola.escola_coorwhatsapp}}"/>
													<input id="escolaProfessorEdi" name="escolaProfessorEdi" value="{{escola.escola_professor}}"/>
													<input id="escolaProfwhatsappEdi" name="escolaProfwhatsappEdi" value="{{escola.escola_profwhatsapp}}"/>
													<input id="escolaFixoEdi" name="escolaFixoEdi" value="{{escola.escola_fixo}}"/>
													<input id="escolaObsEdi" name="escolaObsEdi" value="{{escola.escola_obs}}"/>
													<input id="escolaDataedi" name="escola_dataedi" value="{{escola.escola_dataedi}}"/>
												</div>
											
											</div>
										</div>
									</div>
								</div>
							
							
								<div class="space-4"></div>							
								<div class="space-4"></div>
	
									<div class="form-actions">
										<div class="col-xs-12" style="text-align: center;" >
										<button class="btn btn-info"  name="acao" type="submit">
											<i class="ace-icon fa fa-check bigger-110"></i>
											Salvar
										</button>
	
										&nbsp; &nbsp; &nbsp;
										<button class="btn btn-warning"  href="/menu/ferramentas/escola/lis">
											<i class="ace-icon fa fa-undo bigger-110"></i>
											Voltar
										</button>
									</div>
								</div>

							</form>


						</div><!-- /.col -->
					</div><!-- /.row -->




				</div>
			</div><!-- /.main-content -->


		</div><!-- /.main-container -->





		<!-- basic scripts -->

	</body>
</html>
