<!DOCTYPE html>
<html lang="pt-br"><head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta charset="utf-8">
		<title>Lista de Sessões por Beneficiários</title>
	</head>
	<body onload="javascript:controlaNave();" class="no-skin">
		<div class="main-container ace-save-state" id="main-container">
			<div class="main-content">
				<div class="main-content-inner">
					<div class="breadcrumbs ace-save-state" id="breadcrumbs">
						<ul class="breadcrumb">
							<li>
								<i class="ace-icon fa fa-home home-icon"></i>
								<a href="#">Home</a>
							</li>
							<li class="active">Lista de Beneficiários</li>
						</ul><!-- /.breadcrumb -->
					</div>
					{{#each error}}
						<div class="alert alert-danger">{{texto}}{{error}}</div>
					{{else}}
					{{/each}}
					<div class="page-content"><!-- .page-content -->
						<div class="row"><!--.row -->
							<div class="col-xs-12 col-sm-12">
								<!-- PAGE CONTENT BEGINS -->
								{{#each benes}}
									<div class="widget-box collapsed">
										<div class="widget-header">
											<h6 class="widget-title">{{bene_nome}}</h6>
											<span class="widget-toolbar">
												<a href="#" data-action="reload">
													<i class="ace-icon fa fa-refresh"></i>
												</a>
												<a href="#" data-action="collapse">
													<i class="ace-icon fa fa-chevron-up"></i>
												</a>
											</span>
										</div>
										<div class="widget-body">
											<div class="widget-main">
												<table id="dynamic-table" class="table table-striped table-bordered table-hover dataTable no-footer" role="grid" aria-describedby="dynamic-table_info">
													<thead>
														<tr role="row">
															<th class="sorting" tabindex="0" aria-controls="dynamic-table" rowspan="1" colspan="1" >Terapia</th>
															<th class="sorting" tabindex="1" aria-controls="dynamic-table" rowspan="1" colspan="1" >Liminar</th>
															<th class="sorting" tabindex="2" aria-controls="dynamic-table" rowspan="1" colspan="1" >Realizado</th>
															<th class="sorting" tabindex="3" aria-controls="dynamic-table" rowspan="1" colspan="1" >Saldo</th>
															<th>Ação</th>
														</tr>
													</thead>
													<tbody>
														{{#each ../sessaos}}
															{{#ifEqual this.sessao_beneid ../_id}}
																<tr role="row" class="odd">
																	{{#each ../../terapias}}<!-- Ok -->
																		{{#ifEqual this._id ../sessao_terapiaid01}}
																			<td>{{terapia_nome}}</td>
																		{{/ifEqual}}
																	{{/each}}
																	<td>{{sessao_qtterapiaprev01}}</td>
																	<td>{{sessao_qtterapiacon01}}</td>
																	<td>{{sessao_qtterapiasaldo01}}</td>
																	<td>
																		<div class="action-buttons">
																			<a class="green" name="acaoEdi" id="acaoEdi" href="/menu/beneficiario/sessao/edi/{{_id}}">
																				<i class="ace-icon fa fa-pencil bigger-130"></i>
																			</a>
																			<a class="blue" name="acaolis" id="acaolis" href="/menu/beneficiario/sessao/lis">
																				<i class="ace-icon fa fa-reply bigger-130"></i>
																			</a>
																			<a class="red" name="acaoDel" id="acaoDel" href="/menu/beneficiario/sessao/lisdel/{{_id}}">
																				<i class="ace-icon fa fa-trash-o bigger-130"></i>
																			</a>
																		</div>
																		<div class="hidden-md hidden-lg">
																		</div>
																	</td>
																</tr>
															{{/ifEqual}}
														{{/each}}
													</tbody>
												</table>
											</div>
										</div>
									</div>
								</div>
								{{/each}}<!-- PAGE CONTENT END -->		
							</div>
						</div><!-- /.row -->
					</div><!-- /.page-content -->
				</div><!-- /.main-content-inner -->
			</div><!-- /.main-content -->
		</div><!-- /.main-container ace-save-state --><!-- basic scripts --><!--[if !IE]> -->
		<script src="../../../assets/js/jquery-2.1.4.min.js"></script>
		<script type="text/javascript">
			if('ontouchstart' in document.documentElement) document.write("<script src='../../../assets/js/jquery.mobile.custom.min.js'>"+"<"+"/script>");
		</script>
		<!-- basic scripts -->

		<!-- page specific plugin scripts -->
		<script type="text/javascript" charset="utf8" src="/js/jquery.dataTables.js"></script>
		

		<!-- inline scripts related to this page -->
		<script type="text/javascript">
		$(document).ready(function() {
			$("#dynamic-table").DataTable({
			"language": { "url": "/js/lang/datatable-pt-br.json" },
			"aaSorting": [],
			"aoColumns": [null, null,null,null,{ "bSortable": false }],
			"responsive": true,
			"lengthMenu": [ 100, 200],
			"lengthChange": true,
			"autoWidth": false,
			"paging": true,
			"lengthChange": true,
			"searching": true,
			"ordering": true,
			"info": true,
			
			select: {
						style: 'multi'
					}
			});
		});
		$.fn.dataTable.Buttons.defaults.dom.container.className = 'dt-buttons btn-overlap btn-group btn-overlap';
		</script>
	

	
	</body>

</html>